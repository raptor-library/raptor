DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "RAPtor"
PROJECT_NUMBER         =
PROJECT_BRIEF          = "General, high performance algebraic multigrid solver"
PROJECT_LOGO           = "@CMAKE_CURRENT_SOURCE_DIR@/docs/logo/raptor-logo2-small.png"
OUTPUT_DIRECTORY       = "@CMAKE_CURRENT_BINARY_DIR@/docs/doxygen"
ABBREVIATE_BRIEF       = "The $name class" \
                         "The $name widget" \
                         "The $name file" \
                         is \
                         provides \
                         specifies \
                         contains \
                         represents \
                         a \
                         an \
                         the
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
INPUT                  = "@CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/raptor_main.dox" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/aggregation/" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/core" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/external" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/gallery" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/krylov" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/multilevel" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/profiling" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/ruge_stuben" \
                         "@CMAKE_CURRENT_SOURCE_DIR@/raptor/util"
FILE_PATTERNS          = *.hpp
VERBATIM_HEADERS       = NO
GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTMOL_FIL_EXTENSION    = .xhtml
HTML_EXTRA_STYLESHEET  = @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-awesome.css
HTML_EXTRA_FILES       = @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-awesome-darkmode-toggle.js \
                         @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-awesome-fragment-copy-button.js \
                         @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-awesome-paragraph-link.js \
                         @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-custom/toggle-alternative-theme.js \
                         @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen/doxygen-awesome/doxygen-awesome-interactive-toc.js
HTML_COLORSTYLE        = LIGHT
HIDE_UNDOC_RELATIONS   = YES
HAVE_DOT               = YES
DOT_COMMON_ATTR        = "fontname=Courier,fontsize=10"
DOT_EDGE_ATTR          = "labelfontname=Courier,labelfontsize=10"
DOT_NODE_ATTR          = "shape=box,height=0.2,width=0.4"
DOT_WRAP_THRESHOLD     = 25
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
DOT_IMAGE_FORMAT       = svg
INTERACTIVE_SVG        = YES
